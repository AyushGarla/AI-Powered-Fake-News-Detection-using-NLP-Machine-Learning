        "def nlp_preprocess_text(text: str):\n",
        "    tokens = word_tokenize(text)                          # tokenize\n",
        "    tokens = [t for t in tokens if t not in STOPWORDS]    # remove stopwords\n",
        "    pos_tags = pos_tag(tokens)                            # lemmatize with POS\n",
